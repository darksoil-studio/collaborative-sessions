{
	"name": "example-zome",
	"version": "0.500.0",
	"scripts": {
		"start": "vite --clearScreen false --port $UI_PORT",
		"build": "pnpm lint && tsc && pnpm localize:build && pnpm analyze",
		"lint": "eslint src && prettier \"src/**/*.ts\" --check --ignore-path .gitignore",
		"format": "eslint src --fix && prettier \"src/**/*.ts\" --write --ignore-path .gitignore",
		"localize:extract": "lit-localize extract",
		"localize:build": "lit-localize build",
		"analyze": "cem analyze --litelement --exclude dist"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": ["dist", "locales", "src"],
	"exports": {
		".": "./dist/index.js",
		"./dist/*": "./dist/*",
		"./locales/*": "./locales/*"
	},
	"dependencies": {
		"@automerge/automerge": "^2.0.0",
		"@automerge/prosemirror": "^0.0.13",
		"@darksoil-studio/automerge-real-time-sessions": "workspace:^0.500.0",
		"@darksoil-studio/holochain-elements": "^0.500.0",
		"@darksoil-studio/holochain-signals": "^0.500.0",
		"@darksoil-studio/holochain-utils": "^0.500.0",
		"@darksoil-studio/profiles-provider": "^0.500.0",
		"@darksoil-studio/real-time-sessions-zome": "workspace:^0.500.0",
		"@holochain/client": "^0.19.0",
		"@lit/context": "^1.0.0",
		"@lit/localize": "^0.12.0",
		"@mdi/js": "^7.2.0",
		"@msgpack/msgpack": "^2.8.0",
		"@shoelace-style/shoelace": "^2.11.0",
		"lit": "^3.0.0",
		"prosemirror-example-setup": "^1.2.2",
		"prosemirror-menu": "^1.2.5",
		"prosemirror-model": "^1.21.0",
		"prosemirror-state": "^1.4.3",
		"prosemirror-transform": "^1.9.0",
		"prosemirror-view": "^1.33.6"
	},
	"devDependencies": {
		"@custom-elements-manifest/analyzer": "^0.10.3",
		"@darksoil-studio/profiles-zome": "0.500.0-rev.5f2939510eb1f1cfde83af3c0b460bf5eea03b87",
		"@lit/localize-tools": "^0.6.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^4.0.0",
		"vite-plugin-wasm": "^3.3.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"customElements": "custom-elements.json",
	"type": "module"
}
